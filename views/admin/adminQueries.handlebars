<div class="dashboard-queries">
    <div class="adminQueriesTitle">
        <h1>Consultas: </h1>
    </div>
    {{#each resumeAdmInfo}}
    <input type="hidden" value="{{id}}">
    {{/each}}
    <div class="allqueries">
        <ul>
            {{#each groupedSchedules}}
            <li class="queries">
                <h2>{{@key}}</h2>
                <ul>
                    {{#each this}}
                    <li class="queriesHours">
                        <div class="hourQuery">
                            <h3>{{hour}} </h3>
                        </div>
                        <div class="userQuery">
                            <h3 class="res">Usuario:</h3>
                            <h3>{{userName}}</h3>
                        </div>
                        <div class="petNameQuery">
                            <h3 class="res">Pet:</h3>
                            <h3>{{petName}}</h3>
                        </div>
                        <div class="toFillIn">
                            {{#if (checkIfAdm userName)}}
                            <h3>Horário indisponível</h3>
                            {{else}}
                            <a href="/registerVeterinaryRecord/{{id}}">Preencher Ficha Veterinaria</a>
                            {{/if}}
                        </div>
                    </li>
                    {{/each}}
                </ul>
            </li>
            {{/each}}
        </ul>
    </div>
    <a href="/loading" class="btn"> Clique para acessar o whatsApp</a>
</div>